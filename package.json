{
  "name": "playwrightcore",
  "version": "1.0.0",
  "description": "Projeto de automação Playwright com suporte a Allure Reports",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "e2e": "playwright test --ui",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "report:html": "playwright show-report",
    "report:allure": "allure generate -c allure-results -o allure-report",
    "report:allure:open": "allure serve allure-results",
    "prepare": "playwright install --with-deps"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@types/node": "^24.2.1",
    "allure-commandline": "^2.34.1",
    "allure-js-commons": "^2.14.0",
    "allure-playwright": "^3.3.3",
    "playwright": "^1.54.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@faker-js/faker": "^9.9.0",
    "pdf-parse": "^1.1.1"
  }
}
